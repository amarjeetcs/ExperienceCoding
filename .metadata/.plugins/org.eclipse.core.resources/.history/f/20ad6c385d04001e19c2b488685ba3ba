package com.amarjeet.common;

public class StringBufferPractical {
	public static void main(String[] args) {
		StringBuffer sb=new StringBuffer("amarjeet");
		System.out.println(sb.capacity());
		
		System.out.println(sb.length());
		System.out.println(sb.append(" Singh"));
		System.out.println(sb.delete(0, 1));
		
	}
}	
